{
  "output_folder": "reports",
  "custom_commands_path": "",
  "custom_assertions_path": "",
  "selenium": {
      "server_path": "${KNE_SELENIUM_SERVER}",
    "log_path": "",
    "host": "127.0.0.1",
    "port": 4444,
    "cli_args": {
      "webdriver.chrome.driver": "",
      "webdriver.gecko.driver": "",
      "webdriver.ie.driver": ""
    }
  },
  "test_settings": {
    "default": {
      "launch_url": "http://localhost",
      "silent": true,
      "disable_colors": false,
      "screenshots": {
        "enabled": false,
        "path": ""
      },
      "desiredCapabilities": {
        "browserName": "firefox",
        "version": "latest",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "marionette": true
      },
      "selenium": {
        "start_process": "${KNE_SELENIUM_START_PROCESS}"
      }
    },
    "saucelabs-travis": {
      "selenium_host": "ondemand.saucelabs.com",
      "selenium_port": 80,
      "username": "${SAUCE_USERNAME}",
      "access_key": "${SAUCE_ACCESS_KEY}",
      "use_ssl": false,
      "startConnect": false,
      "silent": true,
      "output": true,
      "screenshots": {
        "enabled": false,
        "on_failure": true,
        "path": ""
      },
      "desiredCapabilities": {
        "name": "test-firefox",
        "browserName": "firefox",
        "version": "latest",
        "tunnel-identifier": "${TRAVIS_JOB_NUMBER}",
        "marionette": true
      },
      "selenium": {
        "start_process": false
      }
    },
    "saucelabs-local": {
      "selenium_host": "ondemand.saucelabs.com",
      "selenium_port": 80,
      "username": "${SAUCE_USERNAME}",
      "access_key": "${SAUCE_ACCESS_KEY}",
      "use_ssl": false,
      "startConnect": false,
      "silent": true,
      "output": true,
      "screenshots": {
        "enabled": false,
        "on_failure": true,
        "path": ""
      },
      "desiredCapabilities": {
        "name": "test-firefox",
        "browserName": "firefox",
        "version": "latest",
        "marionette": true
      },
      "selenium": {
        "start_process": false
      }
    }
  }
}
